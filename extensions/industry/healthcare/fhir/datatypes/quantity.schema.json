   "$schema":"http://json-schema.org/draft-06/schema#",
   "$id":"https://ns.fhir.org/datatypes/quantity",
   "title":"FHIR Quantity",
   "type":"object",
   "description":"A measured or measurable amount.(This datatype is created as per HL7 FHIR Release 5 specifications)",
   "definitions":{
      "quantity":{
         "properties":{
            "fhir:value":{
               "title":"Value",
               "description":"Numerical Value",
               "type":"number"
            },
            "fhir:comparator":{
               "title":"Comparator",
               "description":"< | <= | >= | > | ad ",
               "type":"string",
               "enum":[
                  "<",
                  "<=",
                  ">=",
                  ">",
                  "=",
                  "ad"
               ],
               "meta:enum":{
                  "<":"<",
                  "<=":"<=",
                  ">=":">=",
                  ">":">",
                  "=":"=",
                  "ad":"Ad"
               }
            },
            "fhir:unit":{
               "title":"Unit",
               "description":"Unit representation",
               "type":"string"
            },
            "fhir:system":{
               "title":"System",
               "description":"System that defines coded unit form",
               "type":"string",
               "format":"uri"
            },
            "fhir:code":{
               "title":"Code",
               "description":"Coded form of the unit",
               "type":"string"
            }
         }
      }
   },
   "allOf":[
      {
         "$ref":"#/definitions/quantity"
      }
   ],
   "meta:status":"experimental"
}

